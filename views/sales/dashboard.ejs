<div class="hol-two-spans row">
    <div class="hol-span">
        <div class="span-1-of-2">
            <span class="span-text">Weekly Sales</span>
            <div class="hol-dash row">
                <div class="dash-of-2">
                    <span class="dash-amt center">$55,900</span>
                </div>
                <div class="dash-of-2"></div>
            </div>
        </div>
    </div>

    <div class="hol-span">
        <div class="span-1-of-2">
            <span class="span-text">Market Share</span>
            <div class="hol-dash row">
                <div class="dash-of-2" style="margin-top:20px">
                    <div class="list-of-2 row">
                        <div class="prd-list">
                            <ul>
                                <li><span class="prds" style="background-color: rgb(0, 92, 197);"></span>Phones</li>
                                <li><span class="prds" style="background-color: rgb(0, 171, 214);"></span>Chargers</li>
                                <li><span class="prds" style="background-color: rgb(228, 228, 228);"></span>Protectors</li>
                                <!-- <li><span class="prds"></span>Covers</li> -->
